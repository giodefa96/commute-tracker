{
  "expo": {
    "name": "Commute Tracker",
    "slug": "commute-tracker",
    "version": "2.0.1",
    "description": "Track your daily commute times with detailed statistics and filters. Monitor departure times, platform arrivals, and calculate average commute durations.",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "commutetracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "primaryColor": "#E6F4FE",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.giodefa96.commutetracker"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.giodefa96.commutetracker",
      "versionCode": 3,
      "permissions": ["VIBRATE"]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ebe7817-e38d-49d0-b393-dfcedd0efc1c"
      }
    }
  }
}
